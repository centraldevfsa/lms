{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'StudentDashboard',\n  data() {\n    return {\n      darkMode: JSON.parse(localStorage.getItem('darkMode')) || false,\n      // Estado inicial do modo\n      user: {\n        name: 'Alessandro Becker',\n        progress: 60,\n        courses: [{\n          id: 1,\n          title: 'Nutrição Funcional',\n          description: 'Aprenda os fundamentos e práticas avançadas.',\n          progress: 75\n        }, {\n          id: 2,\n          title: 'Exames Laboratoriais',\n          description: 'Interpretação completa para diagnósticos.',\n          progress: 50\n        }]\n      }\n    };\n  },\n  methods: {\n    toggleDarkMode() {\n      this.darkMode = !this.darkMode;\n      localStorage.setItem('darkMode', JSON.stringify(this.darkMode));\n      if (this.darkMode) {\n        document.documentElement.classList.add('dark');\n      } else {\n        document.documentElement.classList.remove('dark');\n      }\n    },\n    handleLogout() {\n      localStorage.removeItem('user');\n      this.$store.dispatch('logout');\n      this.$router.push('/entrar');\n    }\n  },\n  mounted() {\n    const darkModeFromStorage = JSON.parse(localStorage.getItem('darkMode'));\n    if (darkModeFromStorage) {\n      this.darkMode = true;\n      document.documentElement.classList.add('dark');\n    } else {\n      document.documentElement.classList.remove('dark');\n    }\n  }\n};","map":{"version":3,"names":["name","data","darkMode","JSON","parse","localStorage","getItem","user","progress","courses","id","title","description","methods","toggleDarkMode","setItem","stringify","document","documentElement","classList","add","remove","handleLogout","removeItem","$store","dispatch","$router","push","mounted","darkModeFromStorage"],"sources":["C:\\lms\\src\\views\\StudentDashboard.vue"],"sourcesContent":["<template>\n  <div\n    v-if=\"user\"\n    :class=\"[\n      'dashboard flex min-h-screen',\n      darkMode ? 'bg-gray-900 text-gray-200' : 'bg-gray-100 text-gray-800',\n    ]\"\n  >\n    <!-- Menu lateral -->\n    <aside\n      :class=\"[darkMode ? 'bg-gray-800' : 'bg-white']\"\n      class=\"w-64 shadow-lg\"\n    >\n      <div class=\"p-6 flex justify-between items-center\">\n        <h1 class=\"text-xl font-bold\">\n          {{ user.role === 'admin' ? 'Admin Dashboard' : 'Aluno Dashboard' }}\n        </h1>\n        <button\n          @click=\"toggleDarkMode\"\n          class=\"p-2 rounded-md bg-gray-200 dark:bg-gray-800 text-gray-800 dark:text-gray-200\"\n        >\n          <i :class=\"isDarkMode ? 'fas fa-sun' : 'fas fa-moon'\"></i>\n        </button>\n      </div>\n      <nav class=\"p-4\">\n        <ul>\n          <li v-if=\"user.role === 'admin'\">\n            <router-link\n              to=\"admin/usuarios\"\n              class=\"block py-2 px-4 rounded hover:bg-gray-700 hover:text-white\"\n            >\n              <i class=\"fas fa-home mr-2\"></i> Gerenciar Usuários\n            </router-link>\n          </li>\n          <li>\n            <router-link\n              to=\"/areadoaluno\"\n              class=\"block py-2 px-4 rounded hover:bg-gray-700 hover:text-white\"\n            >\n              <i class=\"fas fa-home mr-2\"></i> Início\n            </router-link>\n          </li>\n          <li>\n            <router-link\n              to=\"/meus-cursos\"\n              class=\"block py-2 px-4 rounded hover:bg-gray-700 hover:text-white\"\n            >\n              <i class=\"fas fa-book mr-2\"></i> Meus Cursos\n            </router-link>\n          </li>\n          <li>\n            <router-link\n              to=\"/configuracoes\"\n              class=\"block py-2 px-4 rounded hover:bg-gray-700 hover:text-white\"\n            >\n              <i class=\"fas fa-cog mr-2\"></i> Configurações\n            </router-link>\n          </li>\n          <li>\n            <button\n              @click=\"handleLogout\"\n              class=\"block py-2 px-4 w-full text-left rounded hover:bg-gray-700 hover:text-white\"\n            >\n              <i class=\"fas fa-sign-out-alt mr-2\"></i> Logout\n            </button>\n          </li>\n        </ul>\n      </nav>\n    </aside>\n\n    <!-- Conteúdo principal -->\n    <main class=\"flex-1 p-6\">\n      <!-- Saudação personalizada -->\n      <div class=\"mb-6\">\n        <h2 class=\"text-2xl font-semibold\">Bem-vindo, {{ user.name }}!</h2>\n        <div v-if=\"user.role === 'admin'\">\n          <p class=\"text-gray-500\">Visualize e gerencie os dados dos alunos.</p>\n        </div>\n        <div v-else>\n          <p class=\"text-gray-500\">Continue de onde você parou.</p>\n        </div>\n      </div>\n\n      <!-- Progresso Geral -->\n      <div class=\"mb-6\">\n        <h3 class=\"text-lg font-semibold mb-2\">Progresso Geral</h3>\n        <div class=\"w-full bg-gray-300 rounded-full h-4\">\n          <div\n            class=\"bg-blue-600 h-4 rounded-full transition-all duration-300 ease-out\"\n            :style=\"{ width: user.progress + '%' }\"\n          ></div>\n        </div>\n        <p class=\"mt-2 text-sm text-gray-500\">\n          Você completou {{ user.progress }}% de seus cursos.\n        </p>\n      </div>\n\n      <!-- Botão para continuar de onde parou -->\n      <div class=\"mb-6 flex justify-between items-center\">\n        <button\n          class=\"px-6 py-3 bg-blue-600 text-white rounded-md hover:bg-blue-700 transition duration-200\"\n        >\n          Continuar de onde parou\n        </button>\n      </div>\n\n      <!-- Cursos Matriculados -->\n      <section class=\"mb-8\">\n        <h3 class=\"text-xl font-semibold mb-4\">Meus Cursos</h3>\n        <div class=\"grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4\">\n          <div\n            v-for=\"course in user.courses\"\n            :key=\"course.id\"\n            class=\"bg-white rounded-lg shadow-md p-4 hover:shadow-lg transition\"\n          >\n            <h4 class=\"font-semibold text-lg\">{{ course.title }}</h4>\n            <p class=\"text-sm text-gray-600\">{{ course.description }}</p>\n            <div class=\"w-full bg-gray-300 rounded-full h-2 mt-2\">\n              <div\n                class=\"bg-blue-600 h-2 rounded-full\"\n                :style=\"{ width: course.progress + '%' }\"\n              ></div>\n            </div>\n            <router-link\n              :to=\"`/cursos/${course.id}`\"\n              class=\"block mt-2 text-blue-500 hover:underline\"\n            >\n              Acessar Curso\n            </router-link>\n          </div>\n        </div>\n      </section>\n\n      <!-- Gamificação -->\n      <section class=\"mb-8\">\n        <h3 class=\"text-xl font-semibold mb-4\">Gamificação</h3>\n        <div\n          class=\"bg-yellow-100 rounded-lg shadow-md p-4 flex items-center gap-4\"\n        >\n          <i class=\"fas fa-trophy text-yellow-500 text-4xl\"></i>\n          <div>\n            <h4 class=\"font-semibold text-lg\">Parabéns!</h4>\n            <p class=\"text-sm text-gray-600\">\n              Você ganhou uma medalha por completar 50% do curso \"Nutrição\n              Funcional\".\n            </p>\n          </div>\n        </div>\n      </section>\n\n      <!-- Links úteis -->\n      <section>\n        <h3 class=\"text-xl font-semibold mb-4\">Links Úteis</h3>\n        <div class=\"grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4\">\n          <router-link\n            to=\"/suporte\"\n            class=\"block bg-white rounded-lg shadow-md p-4 hover:shadow-lg transition\"\n          >\n            <i class=\"fas fa-life-ring text-blue-500 text-xl mb-2\"></i>\n            <h4 class=\"font-semibold\">Suporte</h4>\n            <p class=\"text-sm text-gray-600\">\n              Entre em contato com nossa equipe.\n            </p>\n          </router-link>\n\n          <router-link\n            to=\"/faq\"\n            class=\"block bg-white rounded-lg shadow-md p-4 hover:shadow-lg transition\"\n          >\n            <i class=\"fas fa-question-circle text-blue-500 text-xl mb-2\"></i>\n            <h4 class=\"font-semibold\">FAQ</h4>\n            <p class=\"text-sm text-gray-600\">Perguntas frequentes.</p>\n          </router-link>\n        </div>\n      </section>\n    </main>\n  </div>\n  <div v-else class=\"min-h-screen flex justify-center items-center\">\n    <p class=\"text-gray-500\">Carregando...</p>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'StudentDashboard',\n  data() {\n    return {\n      darkMode: JSON.parse(localStorage.getItem('darkMode')) || false, // Estado inicial do modo\n      user: {\n        name: 'Alessandro Becker',\n        progress: 60,\n        courses: [\n          {\n            id: 1,\n            title: 'Nutrição Funcional',\n            description: 'Aprenda os fundamentos e práticas avançadas.',\n            progress: 75,\n          },\n          {\n            id: 2,\n            title: 'Exames Laboratoriais',\n            description: 'Interpretação completa para diagnósticos.',\n            progress: 50,\n          },\n        ],\n      },\n    };\n  },\n  methods: {\n    toggleDarkMode() {\n      this.darkMode = !this.darkMode;\n      localStorage.setItem('darkMode', JSON.stringify(this.darkMode));\n      if (this.darkMode) {\n        document.documentElement.classList.add('dark');\n      } else {\n        document.documentElement.classList.remove('dark');\n      }\n    },\n    handleLogout() {\n      localStorage.removeItem('user');\n      this.$store.dispatch('logout');\n      this.$router.push('/entrar');\n    },\n  },\n  mounted() {\n    const darkModeFromStorage = JSON.parse(localStorage.getItem('darkMode'));\n    if (darkModeFromStorage) {\n      this.darkMode = true;\n      document.documentElement.classList.add('dark');\n    } else {\n      document.documentElement.classList.remove('dark');\n    }\n  },\n};\n</script>\n\n<style scoped>\n.dashboard {\n  display: flex;\n  min-height: 100vh;\n}\n\naside {\n  width: 16rem;\n}\n\nmain {\n  flex: 1;\n  padding: 1.5rem;\n}\n</style>\n"],"mappings":";AAuLA,eAAe;EACbA,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC,KAAK,KAAK;MAAE;MACjEC,IAAI,EAAE;QACJP,IAAI,EAAE,mBAAmB;QACzBQ,QAAQ,EAAE,EAAE;QACZC,OAAO,EAAE,CACP;UACEC,EAAE,EAAE,CAAC;UACLC,KAAK,EAAE,oBAAoB;UAC3BC,WAAW,EAAE,8CAA8C;UAC3DJ,QAAQ,EAAE;QACZ,CAAC,EACD;UACEE,EAAE,EAAE,CAAC;UACLC,KAAK,EAAE,sBAAsB;UAC7BC,WAAW,EAAE,2CAA2C;UACxDJ,QAAQ,EAAE;QACZ,CAAC;MAEL;IACF,CAAC;EACH,CAAC;EACDK,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACZ,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9BG,YAAY,CAACU,OAAO,CAAC,UAAU,EAAEZ,IAAI,CAACa,SAAS,CAAC,IAAI,CAACd,QAAQ,CAAC,CAAC;MAC/D,IAAI,IAAI,CAACA,QAAQ,EAAE;QACjBe,QAAQ,CAACC,eAAe,CAACC,SAAS,CAACC,GAAG,CAAC,MAAM,CAAC;MAChD,OAAO;QACLH,QAAQ,CAACC,eAAe,CAACC,SAAS,CAACE,MAAM,CAAC,MAAM,CAAC;MACnD;IACF,CAAC;IACDC,YAAYA,CAAA,EAAG;MACbjB,YAAY,CAACkB,UAAU,CAAC,MAAM,CAAC;MAC/B,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,QAAQ,CAAC;MAC9B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,SAAS,CAAC;IAC9B;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,mBAAkB,GAAI1B,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC,CAAC;IACxE,IAAIuB,mBAAmB,EAAE;MACvB,IAAI,CAAC3B,QAAO,GAAI,IAAI;MACpBe,QAAQ,CAACC,eAAe,CAACC,SAAS,CAACC,GAAG,CAAC,MAAM,CAAC;IAChD,OAAO;MACLH,QAAQ,CAACC,eAAe,CAACC,SAAS,CAACE,MAAM,CAAC,MAAM,CAAC;IACnD;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}